<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ga.com.eureka.common.model.mapper.mapper.BaseUserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ga.com.eureka.common.model.mapper.entity.BaseUser">
        <id column="user_id" property="userId" />
        <result column="user_name" property="userName" />
        <result column="user_pwd" property="userPwd" />
        <result column="user_phone" property="userPhone" />
        <result column="user_gender" property="userGender" />
        <result column="user_age" property="userAge" />
        <result column="user_active" property="userActive" />
        <result column="create_dt" property="createDt" />
        <result column="update_dt" property="updateDt" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        user_id, user_name, user_pwd, user_phone, user_gender, user_age, user_active, create_dt, update_dt
    </sql>

</mapper>
